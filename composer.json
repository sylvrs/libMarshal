{
  "name": "sylvrs/libmarshal",
  "description": "A library used for marshalling/unmarshalling data into objects",
  "minimum-stability": "stable",
  "license": "gpl-3.0",
  "version": "2.0.0",
  "authors": [
    {
      "name": "Matthew Jordan",
      "email": "personal@matthewjordan.xyz"
    }
  ],
  "autoload": {
    "psr-4": {
      "libMarshal\\": "src/libMarshal"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "libMarshal\\": "tests/libMarshal"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-yaml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.17.0",
    "phpunit/phpunit": "^10.1.3",
    "phpstan/phpstan": "1.10.15",
    "phpbench/phpbench": "^1.2",
    "netresearch/jsonmapper": "^4.2",
    "cuyz/valinor": "^1.4",
    "json-mapper/json-mapper": "^2.18"
  },
  "extra": {
    "virion":{
      "spec": "3.0",
      "namespace-root": "libMarshal\\"
    }
  }
}
